{
    "jsc": {
      "parser": {
        "syntax": "typescript",
        "tsx": true,
        "decorators": true
      },
      "transform": {
        "react": {
          "runtime": "automatic"
        },
        "legacyDecorator": true,
        "useDefineForClassFields": true
      },
      "target": "es2022",
      "loose": false
    },
    "module": {
      "type": "es6",
      "strict": true,
      "noInterop": false
    },
    "minify": false,
    "sourceMaps": true,
    "extensions": {
      "styledJsx": true
    },
    "plugins": [
      [
        "@necto/swc/plugins/styled-jsx",
        {}
      ]
    ]
  }
  